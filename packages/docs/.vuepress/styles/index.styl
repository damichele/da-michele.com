.menulink
  margin 1rem 0
  padding .1rem 1.5rem
  a
    background-color var(--blockquoteBorderColor)
    border-radius 4px
    color var(--textColor)
    display inline-block
    font-size 1.2rem
    padding 0 .8rem
    transition background-color .1s ease
    box-sizing border-box
    .title
      font-weight bold
    p
      font-weight bold

.teammember
  margin 1rem 0
  padding .1rem 1.5rem
  color var(--base01)
  background-color var(--base06)
  border-radius 0.4rem
  .title
    font-weight bold

.custom-block
  &.right
    color rgba($textColor, 0.4)
    font-size 0.9rem
    text-align right
    @media (prefers-color-scheme dark)
      color rgba($textDarkColor, 0.4)

.theme-container
  &.custom-menu
    h1, h2, h3, h4, h5, h6, p, ul, ol
      font-family 'Lato'
      font-size-adjust 1
    img
      margin 0.5rem 0.5rem 0rem 0.5rem
      box-sizing border-box
      border-radius 4px

